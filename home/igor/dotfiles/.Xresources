!! URxvt Appearance
URxvt.font: xft:Monospace:style=Regular:size=12
URxvt.boldFont: xft:Monospace:style=Bold:size=12
URxvt.italicFont: xft:Monospace:style=Italic:size=12
URxvt.boldItalicfont: xft:Monospace:style=BoldItalic:size=12
URxvt.letterSpace: 0
URxvt.lineSpace: 0
!URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
!URxvt.depth: 24
URxvt.iso14755: false

!! Extensions
URxvt.perl-ext-common:	default,matcher,keyboard-select,font-size,-confirm-paste
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.C-j: perl:keyboard-select:activate
URxvt.keyboard-select.clipboard: true
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
URxvt.iso14755_52: false
!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1

!! Paste 
URxvt.keysym.Control-y: eval:paste_clipboard

rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

URxvt.transparent: true
